{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-dom": "*",
        "ext-imagick": "*",
        "ext-imap": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-ssh2": "*",
        "composer/composer": "^2.7",
        "guzzlehttp/guzzle": "^7.8",
        "inertiajs/inertia-laravel": "^2.0",
        "kregel/exception-probe": "^1.0",
        "kregel/laravel-flight": "^1.0",
        "laravel/framework": "^12.7",
        "laravel/horizon": "^5.30",
        "laravel/jetstream": "^5.3",
        "laravel/nova": "^5.7",
        "laravel/reverb": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.12",
        "league/flysystem-ftp": "^3.0",
        "lesstif/php-jira-rest-client": "^5.7",
        "meilisearch/meilisearch-php": "^1.6",
        "mikehaertl/php-pdftk": "^0.13.1",
        "mustache/mustache": "^2.14",
        "nette/php-generator": "*",
        "php-imap/php-imap": "^5.0",
        "phpseclib/phpseclib": "*",
        "picqer/php-barcode-generator": "^3.2",
        "pusher/pusher-php-server": "^7.0",
        "smalot/pdfparser": "^2.10",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-ignition": "^2.9",
        "spatie/laravel-permission": "^6.12",
        "spatie/laravel-query-builder": "^6.3",
        "spatie/laravel-responsecache": "^7.7",
        "spatie/laravel-tags": "^4.9",
        "spatie/nova-tags-field": "^5.0",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-json-relations": "^1.14",
        "tightenco/ziggy": "^2.0",
        "toin0u/digitalocean-v2": "^5.0",
        "kregel/nova-banking": "@dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.1",
        "laravel/nova-devtool": "^1.8",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nikic/php-parser": "^5.4",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "ryoluo/sail-ssl": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan optimize:clear"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan optimize:clear"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        "0": {
            "type": "path",
            "url": "./nova-components/NovaBanking"
        }
    }
}
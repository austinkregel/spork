{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-dom": "*",
        "ext-imagick": "*",
        "ext-imap": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-ssh2": "*",
        "bugsnag/bugsnag-laravel": "^2.27",
        "composer/composer": "^2.6",
        "guzzlehttp/guzzle": "^7.7",
        "inertiajs/inertia-laravel": "^1.0",
        "kregel/exception-probe": "^1.0",
        "kregel/laravel-flight": "^1.0",
        "laravel-notification-channels/webhook": "^2.6",
        "laravel/forge-sdk": "^3.13",
        "laravel/framework": "^11.7",
        "laravel/horizon": "^5.22",
        "laravel/jetstream": "^5.0",
        "laravel/pulse": "^1.2",
        "laravel/reverb": "^1.3",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.8",
        "laravel/tinker": "^2.9",
        "league/flysystem-ftp": "^3.0",
        "lesstif/php-jira-rest-client": "^5.7",
        "meilisearch/meilisearch-php": "^1.6",
        "mikehaertl/php-pdftk": "^0.13.1",
        "mustache/mustache": "^2.14",
        "nativephp/electron": "^0.5.0",
        "nette/php-generator": "*",
        "php-imap/php-imap": "^5.0",
        "phpseclib/phpseclib": "*",
        "picqer/php-barcode-generator": "^2.4",
        "pusher/pusher-php-server": "^7.0",
        "smalot/pdfparser": "^2.10",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-ignition": "^2.4",
        "spatie/laravel-permission": "^6.2",
        "spatie/laravel-query-builder": "^5.8",
        "spatie/laravel-tags": "^4.6",
        "staudenmeir/eloquent-json-relations": "^1.11",
        "tightenco/ziggy": "^2.0",
        "toin0u/digitalocean-v2": "^4.9",
        "winter/laravel-config-writer": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan optimize:clear"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan optimize:clear"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
